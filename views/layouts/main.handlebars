<!DOCTYPE html>
<html lang="en">

<head>
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({
            google_ad_client: "ca-pub-2171615785310933",
            enable_page_level_ads: true
        });
    </script>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="{{#if post}}MapleStory2 {{ capitalize post.postType}}: {{post.description}} - {{gstatic.description}}{{else}}{{gstatic.description}}{{/if}}">
    <meta name="robots" content="index, follow">
    <meta http-equiv="X-UA-Compatible">
    <title>{{title}}</title>

    <!--OpenGraph-->
    <meta property="og:title" content="{{title}}" />
    <meta property="og:type" content="website" />
    <meta property="og:image" content="/assets/graph.png" />
    <meta property="og:site_name" content="MS2World" />
    <meta property="og:description" content="{{#if post}}MapleStory2 {{ capitalize post.postType}}: {{post.description}}{{else}}{{gstatic.description}}{{/if}}" />

    <!--Twitter-->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:title" content="{{title}}">
    <meta name="twitter:description" content="{{#if post}}MapleStory2 {{ capitalize post.postType}}: {{post.description}}{{else}}{{gstatic.description}}{{/if}}">
    <meta name="twitter:image" content="/assets/graphtwitter.png">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/css/bootstrap.css">
    <!-- MDI CSS -->
    <link rel="stylesheet" href="https://cdn.materialdesignicons.com/2.8.94/css/materialdesignicons.min.css">
    <!-- jQuery 3.3.1 -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.2.1/dist/jquery.min.js"></script>
    <!-- PopperJS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49"
        crossorigin="anonymous"></script>
    <!-- Bootstrap JS -->
    <script src="/js/bootstrap.js"></script>
    <!-- StickyJS -->
    <script src="/js/jquery.sticky.js"></script>
    <!-- timeagoJS-->
    <script src="https://cdn.jsdelivr.net/npm/timeago@1.6.3/jquery.timeago.min.js" type="text/javascript"></script>
    <!--CookiesJS-->
    <script src="https://cdn.jsdelivr.net/npm/js-cookie@2/src/js.cookie.min.js"></script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-129274603-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-129274603-1');
    </script>


    {{#if cstatic}}
    {{#each cstatic}}
    {{{this}}}
    {{/each}}
    {{/if}}
</head>

<body>
    <div id="adblockmessage">
    </div>

    <div class="container-fluid p-0">
        <div class="bg-secondary text-center text-white">
            <div class="donor-messages-loading">
                <a href="/donate" title="Donor message" class="text-white">üì¢</a> <a target="blanc" href="/donate"
                    class="text-white">Loading...</a>
            </div>
            {{#each messages}}
            <div class="donor-messages" style="display:none">
                <a href="/donate" title="Donor message" class="text-white">üì¢</a> <a target="blanc" href="{{link}}"
                    class="text-white">{{message}}</a>
            </div>
            {{/each}}
        </div>
    </div>
    <header class="mb-3">
        <div id="brand-logo" class="d-flex justify-content-center mt-3">
            <a href="/home">
                <div style="width: 135px; height: 30px; background:url('/assets/logo.png') transparent center center no-repeat; background-size: 135px 30px;"></div>
            </a>
        </div>
        <nav class="navbar navbar-expand-sm navbar-light bg-white border-bottom shadow-sm" id="header">
            <div class="container">
                <button class="btn btn-dark btn-sm d-sm-none" type="button" data-toggle="collapse" data-target="#navbar"
                    aria-controls="navbar" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="mdi mdi-menu"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbar">
                    <div class="d-flex justify-content-center align-items-center w-100 flex-column flex-sm-row">
                        <a class="pr-3 text-dark" href="/home">Home</a>
                        <a class="pr-3 text-dark" href="/guides">Guides</a>
                        <div class="btn-group" id="dropdownBuild">
                            <a class="dropdown-toggle pr-3 text-dark" href="#" data-toggle="dropdown" aria-haspopup="true"
                                aria-expanded="false">
                                Builds
                            </a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" href="/create-build">Skill Builder <span class="badge badge-secondary">Beta</span></a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" href="/builds">Builds list</a>
                            </div>
                        </div>
                        {{!-- <a class="pr-3 text-dark" href="/lives"><span class="mdi mdi-record text-danger "></span>Lives</a>
                        --}}
                        {{#if user}}
                        <div class="btn-group ml-auto">
                            <button type="button" class="btn btn-dark btn-sm dropdown-toggle" data-toggle="dropdown"
                                aria-haspopup="true" aria-expanded="false">
                                {{user.username}}
                            </button>
                            <div class="dropdown-menu dropdown-menu-right">
                                <a href="/profile/{{user.steamid}}" class="dropdown-item"><span class="mdi mdi-account"></span>
                                    Profile</a>
                                <div class="dropdown-divider"></div>
                                <a href="/create-guide" class="dropdown-item"><span class="mdi mdi-plus-circle"></span>
                                    Create guide</a>
                                <a href="/create-build/knight" class="dropdown-item"><span class="mdi mdi-plus-circle"></span>
                                    Create build</a>
                                <div class="dropdown-divider"></div>
                                <a href="/logout" class="dropdown-item bg-danger text-white"><span class="mdi mdi-logout"></span>
                                    Logout</a>
                            </div>
                        </div>
                        {{else}}
                        <button class="loginbtn btn btn-dark btn-sm ml-auto" data-toggle="tooltip" data-placement="bottom"
                            title="Login with Steam"><span class="mdi mdi-steam"></span>
                            Login</button>
                        {{/if}}
                    </div>
                </div>
            </div>
        </nav>
    </header>

    {{{body}}}

    <div class="container">
        <footer class="pt-4 my-md-5 pt-md-5 border-top">
            <div class="row">
                <div class="col-12 col-md text-cenetr my-auto">
                    <small class="d-block mb-3 text-muted">&copy; {{year}} - Maded with ‚ù§ by <a href="https://twitter.com/jecsham">Jecsham</a></small>
                </div>
                <div class="col-6 col-md">
                    <h5>Cool ms2 sites</h5>
                    <ul class="list-unstyled text-small">
                        <li><a class="text-muted" href="https://ms2codex.com/us/" target="_blanc">ms2codex</a></li>
                        <li><a class="text-muted" href="https://musicalnexus.net/" target="_blanc">musicalnexus</a></li>
                    </ul>
                </div>
                <div class="col-6 col-md">
                    <h5>Resources</h5>
                    <ul class="list-unstyled text-small">
                        <li><a class="text-muted" href="https://github.com/jecsham/ms2world" target="blanc">Source Code</a></li>
                        <li><a class="text-muted" href="http://maplestory2.nexon.net/" target="_blanc">MapleStory2</a></li>
                        <li><a class="text-muted" href="https://store.steampowered.com/app/560380/MapleStory_2/" target="_blanc">MapleStory2
                                in Steam</a></li>
                        <li><a class="text-muted" href="/donate">Donate</a></li>
                    </ul>
                </div>
                <div class="col-6 col-md">
                    <h5>About</h5>
                    <ul class="list-unstyled text-small">
                        <li><a class="text-muted" href="/privacy">Privacy</a></li>
                        <li><a class="text-muted" href="/contact">Contact</a></li>
                    </ul>
                </div>
            </div>
        </footer>
    </div>
    <div id="cookie-notice" class="container-fluid position-fixed fixed-bottom">
        <div class="row">
            <div class="col">
                <div class="alert alert-info alert-dismissible fade" role="alert" id="cookies-alert"><strong>Cookies
                        Notice üç™!
                    </strong> We use cookies and other tracking technologies to improve your browsing
                    experience on our website, to show you personalized content and targeted ads, to analyze our
                    website traffic, and to understand where our visitors are coming from. By browsing our website, you
                    consent to our use of cookies and other tracking technologies.
                    <a href="#" class="text-dark  float-right" data-dismiss="alert" aria-label="Close">
                        I Agree
                        <span aria-hidden="true"></span>
                    </a>
                </div>
            </div>
        </div>
    </div>
    {{> modals/loginmodal}}
    <script src="/js/global.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/blockadblock/3.2.1/blockadblock.js"></script>
    <script>

        function adBlockDetected() {
            //$('#adblockmessage').html('<div class="alert alert-danger text-center alert-dismissible fade show" role="alert">AdBlocker enabled üò¢<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>');
        }
        function adBlockNotDetected() {
            $('#adblockmessage').hide();
        }

        if (typeof blockAdBlock === 'undefined') {
            adBlockDetected();
        } else {
            blockAdBlock.setOption({ debug: false });
            blockAdBlock.onDetected(adBlockDetected).onNotDetected(adBlockNotDetected);
        }
        $('.donor-messages-loading').hide()
        var quotes = $(".donor-messages");
        var quoteIndex = -1;

        function showNextQuote() {
            ++quoteIndex;
            quotes.eq(quoteIndex % quotes.length)
                .fadeIn(2000)
                .delay(10000)
                .fadeOut(2000, showNextQuote);
        }
        showNextQuote();

    </script>
</body>

</html>